<?xml version="1.0"?>
<launch>
  <arg name="bagfile" />
  <param name="use_sim_time" value="true" />

  <include file="$(find ep_driver)/launch/ep_description.launch"/>
  <include file="$(find ep_driver)/launch/bringup.launch"/>
  <include file="$(find rmus_cartographer)/launch/modules/mapping.launch" />

  <node name="player" pkg="rosbag" type="play" output="screen" args="--clock --rate=1.0 $(arg bagfile)" />

</launch>
